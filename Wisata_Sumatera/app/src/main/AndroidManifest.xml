    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_time_to_leave_24"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_baseline_time_to_leave_24"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DetailOlahRaga"/>
        <activity android:name=".DetailWisata" />
        <activity android:name=".OlahRagaActivity" />
        <activity android:name=".BiodataActivity"/>
        <activity android:name=".WisataActivity" />
        <activity android:name=".SplashScreen"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">

        </activity>
    </application>

</manifest>